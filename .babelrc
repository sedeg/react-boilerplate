{
  "presets": [
    ["@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    ["react-remove-properties", {"properties": ["data-test"]}],
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
        {
          "useESModules": true
        }
    ]
  ]
}
